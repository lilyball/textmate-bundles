								<key>2</key>
								<dict>
									<key>name</key>
									<string>variable.parameter.handler.applescript</string>
								</dict>
							</dict>
							<key>match</key>
							<string>\b(above|against|apart from|around|aside from|at|below|beneath|beside|between|by|for|from|instead of|into|on|onto|out of|over|thru|under)\s+(\w+)\b</string>
						</dict>
						<dict>
							<key>include</key>
							<string>$self</string>
