<plist version="1.0">
<dict>
	<key>beforeRunningCommand</key>
	<string>nop</string>
	<key>command</key>
	<string>{ osascript -e 'tell app "TextMate" to quit';
  while [[ -n $(ps -xc|grep 'TextMate$') ]]; do
    sleep .1;
  done
  open ~/Oak/TextMate.app; } &amp;&gt;/dev/null &amp;</string>
	<key>input</key>
	<string>none</string>
	<key>keyEquivalent</key>
	<string>@R</string>
	<key>name</key>
	<string>Restart TextMate</string>
	<key>output</key>
	<string>discard</string>
	<key>uuid</key>
