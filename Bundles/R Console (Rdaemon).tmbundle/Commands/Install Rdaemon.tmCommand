	<string>none</string>
	<key>name</key>
	<string>Install Rdaemon</string>
	<key>output</key>
	<string>showAsTooltip</string>
	<key>scope</key>
	<string>source.rd.console</string>
	<key>tabTrigger</key>
	<string>installrd</string>
