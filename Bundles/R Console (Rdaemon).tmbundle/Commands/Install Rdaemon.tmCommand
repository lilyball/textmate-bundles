<dict>
	<key>beforeRunningCommand</key>
	<string>nop</string>
	<key>command</key>
	<string>[[ -d "$HOME"/Rdaemon ]] &amp;&amp; echo -e "'$HOME/Rdaemon' already exists! Please remove it first." &amp;&amp; exit_show_tool_tip
